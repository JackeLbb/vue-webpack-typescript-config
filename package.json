{
  "name": "vue-webpack-4",
  "version": "1.0.0",
  "private": true,
  "description": "A Vuejs template using Webpack 4",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "build": "cross-env-shell NODE_ENV=production npm run clean && npm run lint && npm run test && npm run compile",
    "prod": "webpack --config build/webpack.config.prod.js",
    "compile": "webpack --config config/webpack.config.prod.js",
    "clean": "rimraf dist && rimraf coverage",
    "serve": "http-server dist/ -g -o",
    "lint": "tslint src/**/*.ts",
    "test": "cross-env NODE_ENV=development karma start config/karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start config/karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start config/karma.unit.js --singleRun=false --auto-watch"
  },
  "keywords": [
    "Vue",
    "typescript",
    "axios",
    "scss",
    "Webpack 4.0"
  ],
  "author": "Daniel Cook",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.22",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.6",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.1.0",
    "karma-webpack": "^3.0.5",
    "lodash.merge": "^4.6.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tslint": "~5.9.1",
    "tslint-config-standard": "~7.0.0",
    "tslint-loader": "~3.5.3",
    "typescript": "^3.0.0",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "vue-hot-reload-api": "~2.2.4",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.5.1",
    "vue-server-renderer": "^2.5.22",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
